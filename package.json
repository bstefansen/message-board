    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "helmet": "^3.22.0",
    "yarn": "^1.22.4"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  },
  "keywords": [
    "node",
    "hyperdev",
    "express"
  ],
